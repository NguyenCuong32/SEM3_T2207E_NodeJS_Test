<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"></script>
    <title>Document</title>
  </head>
  <body>
    <form action="/add" method="post">
      <div class="mb-3 mt-3">
        <label for="name" class="form-label">ProductCode:</label>
        <input
          type="text"
          class="form-control"
          id="email"
          placeholder="Enter Product Code"
          name="ProductCode"
        />
      </div>
      <div class="mb-3">
        <label for="pwd" class="form-label">ProductName:</label>
        <input
          type="text"
          class="form-control"
          id="pwd"
          placeholder="Enter Product Name"
          name="ProductName"
        />
      </div>
      <div class="mb-3">
        <label for="pwd" class="form-label">ProductData:</label>
        <input
          type="text"
          class="form-control"
          id="pwd"
          placeholder="Enter Product Data"
          name="ProductData"
        />
      </div>
      <div class="mb-3">
        <label for="pwd" class="form-label">ProductOriginPrice:</label>
        <input
          type="text"
          class="form-control"
          id="pwd"
          placeholder="Enter Product Origin Price"
          name="ProductOriginPrice"
        />
      </div>
      <div class="mb-3">
        <label for="pwd" class="form-label">Quantity:</label>
        <input
          type="text"
          class="form-control"
          id="pwd"
          placeholder="Enter Quantity"
          name="Quantity"
        />
      </div>
      <div class="mb-3">
        <label for="pwd" class="form-label">ProductStoreCode:</label>
        <input
          type="text"
          class="form-control"
          id="pwd"
          placeholder="Enter Product Store Code"
          name="ProductStoreCode"
        />
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </body>
</html>
