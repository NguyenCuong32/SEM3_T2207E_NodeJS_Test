<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Product List</title>
</head>
<body>
  <h1>Product List</h1>
  <ul>
    <% users.forEach(user => { %>
      <li>
        <%= user.username %> - <%= user.fullname %> - <%= user.address %>
        <a href="/edit/<%= user._id %>">Edit</a>
        <a href="/delete/<%= user._id %>">Delete</a>
      </li>
    <% }); %>
  </ul>
  <a href="/create" style="color: brown;">Add Product</a>
</body>
</html>